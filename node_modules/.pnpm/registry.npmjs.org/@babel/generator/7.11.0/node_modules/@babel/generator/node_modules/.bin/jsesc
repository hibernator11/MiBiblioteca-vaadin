#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/home/gustavo/vaadin/tutorial-vaadin/node_modules/.pnpm/registry.npmjs.org/jsesc/2.5.2/node_modules/jsesc/bin/jsesc/node_modules:/home/gustavo/vaadin/tutorial-vaadin/node_modules/.pnpm/registry.npmjs.org/jsesc/2.5.2/node_modules/jsesc/bin/node_modules:/home/gustavo/vaadin/tutorial-vaadin/node_modules/.pnpm/registry.npmjs.org/jsesc/2.5.2/node_modules/jsesc/node_modules:/home/gustavo/vaadin/tutorial-vaadin/node_modules/.pnpm/registry.npmjs.org/jsesc/2.5.2/node_modules:/home/gustavo/vaadin/tutorial-vaadin/node_modules/.pnpm/registry.npmjs.org/jsesc/node_modules:/home/gustavo/vaadin/tutorial-vaadin/node_modules/.pnpm/registry.npmjs.org/node_modules:/home/gustavo/vaadin/tutorial-vaadin/node_modules/.pnpm/node_modules:/home/gustavo/vaadin/tutorial-vaadin/node_modules:/home/gustavo/vaadin/node_modules:/home/gustavo/node_modules:/home/node_modules:/node_modules" "$basedir/node"  "$basedir/../../../../../../../../jsesc/2.5.2/node_modules/jsesc/bin/jsesc" "$@"
  ret=$?
else 
  NODE_PATH="/home/gustavo/vaadin/tutorial-vaadin/node_modules/.pnpm/registry.npmjs.org/jsesc/2.5.2/node_modules/jsesc/bin/jsesc/node_modules:/home/gustavo/vaadin/tutorial-vaadin/node_modules/.pnpm/registry.npmjs.org/jsesc/2.5.2/node_modules/jsesc/bin/node_modules:/home/gustavo/vaadin/tutorial-vaadin/node_modules/.pnpm/registry.npmjs.org/jsesc/2.5.2/node_modules/jsesc/node_modules:/home/gustavo/vaadin/tutorial-vaadin/node_modules/.pnpm/registry.npmjs.org/jsesc/2.5.2/node_modules:/home/gustavo/vaadin/tutorial-vaadin/node_modules/.pnpm/registry.npmjs.org/jsesc/node_modules:/home/gustavo/vaadin/tutorial-vaadin/node_modules/.pnpm/registry.npmjs.org/node_modules:/home/gustavo/vaadin/tutorial-vaadin/node_modules/.pnpm/node_modules:/home/gustavo/vaadin/tutorial-vaadin/node_modules:/home/gustavo/vaadin/node_modules:/home/gustavo/node_modules:/home/node_modules:/node_modules" node  "$basedir/../../../../../../../../jsesc/2.5.2/node_modules/jsesc/bin/jsesc" "$@"
  ret=$?
fi
exit $ret
